<%= form_for @user do |f| %>
  Name: <%= f.text_field :name %><br>
  Email: <%= f.text_field :email %><br>
  Password: <%= f.password_field :password %><br>
  Confirm Password: <%= f.password_field :password_confirmation %><br>
  Phone: <%= f.text_field :phone %><br>
  <%= f.submit "Sign Up" %>
<% end %>
